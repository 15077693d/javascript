{
  "name": "@clerk/backend",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Clerk Backend SDK - REST Client for Backend API & JWT verification utilities",
  "scripts": {
    "prebuild": "run-s clean build:runtime",
    "build": "tsc -p tsconfig.esm.json",
    "build:runtime": "mkdir -p dist/runtime && cp -r src/runtime/* dist/runtime",
    "clean": "rimraf ./dist",
    "lint": "eslint .",
    "dev": "tsc -p tsconfig.esm.json --watch",
    "test": "run-s build:runtime test:node test:edge-runtime",
    "test:node": "./tests/node/run.sh",
    "test:edge-runtime": "./tests/edge-runtime/run.sh",
    "test:cloudflare-workers": "tests/cloudflare-workers/run.sh",
    "test:cloudflare-workers2": "tests/cloudflare-workers/run2.sh",
    "test:cloudflare-workers3": "tests/cf-miniflare/run.sh"
  },
  "module": "dist/mjs/index.js",
  "dependencies": {
    "@clerk/types": "^3.10.1",
    "@types/node": "^16.11.55",
    "node-fetch-native": "^0.1.7",
    "rfc4648": "^1.5.2",
    "snakecase-keys": "^5.4.4",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@types/jest": "^27.4.0",
    "edge-runtime": "^2.0.0",
    "esbuild": "^0.15.12",
    "esbuild-register": "^3.3.3",
    "jest": "^27.4.7",
    "miniflare": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "sinon": "^14.0.1",
    "ts-jest": "^27.1.3",
    "typescript": "*",
    "uvu": "^0.5.6",
    "workerd": "1.20220926.3",
    "wrangler": "^2.1.13"
  },
  "publishConfig": {
    "access": "public"
  },
  "imports": {
    "#crypto": {
      "node": "./dist/runtime/node/crypto.js",
      "default": "./dist/runtime/browser/crypto.js"
    },
    "#fetch": {
      "node": "./dist/runtime/node/fetch.js",
      "default": "./dist/runtime/browser/fetch.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clerkinc/javascript.git"
  },
  "bugs": {
    "url": "https://github.com/clerkinc/javascript/issues"
  },
  "homepage": "https://clerk.dev/",
  "engines": {
    "node": ">=16"
  }
}
